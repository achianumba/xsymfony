{
    "name": "xsymfony",
    "dockerComposeFile": "compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "hostRequirements": {
        "cpus": 2,
        "memory": "8gb",
        "storage": "32gb"
    },
    "remoteUser": "xsymfony",
     "customizations": {
        "vscode": {
            "extensions": [
                "xdebug.php-pack",
                "redhat.vscode-yaml",
                "ms-azuretools.vscode-docker",
                "ms-vscode-remote.remote-containers"
            ],
            "settings": {
                "php.validate.executablePath": "/usr/bin/php",
                "redhat.telemetry.enabled": false
            }
        }
    },
}